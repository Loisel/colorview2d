# Config file for colorview2d (YAML)
# Filename of default view
datafilename: data/demo.dat
# Default colormap
Colormap: jet
# Default labels
Xlabel: x-axis
Ylabel: y-axis
Cblabel: colorscale
# Format of ticklabels
Xtickformat: auto
Ytickformat: auto
Cbtickformat: auto
# Fontstyle and size
Font:  default
Fontsize: 12
# Ticksize
Xticklength: 4
Yticklength: 4
# Linewidth of lines that are drawn in the plot
Linewidth: 1
# Default Size of the plot and dpi
Width: 0
Height: 0
Dpi: 75